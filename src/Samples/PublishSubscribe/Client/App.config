<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="MassTransit" type="MassTransit.Config.Xml.ConfigurationSection, MassTransit" />
  </configSections>

  <MassTransit>
    <ReceiveFrom>
      msmq://localhost/mt_client
    </ReceiveFrom>

    <SubscriptionService>
      msmq://localhost/mt_subscriptions
    </SubscriptionService>

    <HealthService>
      3
    </HealthService>
    
    <Transports>
      <Transport>
        MassTransit.Transports.Msmq.MsmqEndpoint, MassTransit.Transports.Msmq
      </Transport>
    </Transports>
  </MassTransit>
</configuration>